# OpenRouter API Configuration
OPENROUTER_API_KEY=your_openrouter_api_key_here

# Flask Configuration
SECRET_KEY=your_secret_key_here

# Cloudinary Configuration (for file uploads)
CLOUDINARY_CLOUD_NAME=your_cloud_name
CLOUDINARY_API_KEY=your_api_key
CLOUDINARY_API_SECRET=your_api_secret

# NeonDB PostgreSQL Configuration
# Format: postgresql://username:password@host:port/database_name
# Example NeonDB URL: postgresql://username:password@ep-example-123456.us-east-1.aws.neon.tech/chatbot_db
DATABASE_URL=postgresql://username:password@your-neon-host.neon.tech/chatbot_db

# Optional: Additional database configuration
DB_POOL_SIZE=10
DB_MAX_OVERFLOW=20